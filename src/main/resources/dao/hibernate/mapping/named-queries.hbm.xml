<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>

	<!-- ClusterRep -->
	<query name="ClusterRep.findAll">select clusterrep from ClusterRep clusterrep</query>
	<query name="ClusterRep.findByPlanetsAllowed">select clusterrep from ClusterRep clusterrep where clusterrep.planetsAllowed = ?</query>
</hibernate-mapping>